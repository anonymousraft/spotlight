{
    "name": "Spotlight - SEO Suite",
    "description": "Extension for SEO heading and A Tag URL Highligt on Web page",
    "version": "1.0.0",
    "homepage_url": "https://hitendra.co",
    "manifest_version": 2,
    "icons": {
        "16": "./icons/spotlight-16x16.png",
        "32": "./icons/spotlight-32x32.png",
        "48": "./icons/spotlight-48x48.png",
        "128": "./icons/spotlight-128x128.png"
    },
    "background": {
        "scripts": [
            "./js/background.js"
        ]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "./popup.html"
    },
    "permissions": [
        "tabs",
        "https://*/*"
    ]
}